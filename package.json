{
  "name": "pm2-logs-viewer",
  "version": "1.0.0",
  "description": "HTTP server to view PM2 logs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "pm2:start": "pm2 start pm2.config.json",
    "pm2:stop": "pm2 stop pm2-logs-viewer",
    "pm2:restart": "pm2 restart pm2-logs-viewer",
    "pm2:delete": "pm2 delete pm2-logs-viewer",
    "pm2:logs": "pm2 logs pm2-logs-viewer"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
